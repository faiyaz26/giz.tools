{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "parser:build": "npm run build --workspace=packages/markdown-parser",
    "parser:dev": "npm run dev --workspace=packages/markdown-parser",
    "parser:example": "npm run dev --workspace=packages/markdown-parser -- example",
    "parse:python": "npm run dev --workspace=packages/markdown-parser -- example --pretty",
    "parse:all-cheatsheets": "npm run dev --workspace=packages/markdown-parser -- batch 'app/cheatsheets/**/*.md' --pretty",
    "parse:individual": "npm run build --workspace=packages/markdown-parser && cd packages/markdown-parser && node dist/cli.js individual \"examples/*.md\" -o ../../public/data/cheatsheets --index-output ../../public/data/cheatsheets-index.json --pretty",
    "update:cheatsheets": "npm run build --workspace=packages/markdown-parser && cd packages/markdown-parser && node dist/cli.js github -o ../../public/data/cheatsheets --index-output ../../public/data/cheatsheets-index.json --pretty && cd ../.. && cp -r packages/markdown-parser/public/data/* public/data/"
  },
  "dependencies": {
